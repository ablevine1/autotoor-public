{
  "private": true,
  "name": "@autotoor/app-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf build",
    "dev": "expo start",
    "lint": "eslint --ext js,ts,tsx .",
    "build": "expo export --output-dir ./build --platform all",
    "start": "expo start",
    "test": "jest",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eas-build-pre-install": "npm install --global pnpm@7.x",
    "eas-build-post-install": "pnpm run -w build:mobile"
  },
  "dependencies": {
    "@autotoor/ui": "*",
    "expo": "^48.0.4",
    "expo-dev-client": "~2.1.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-get-random-values": "^1.9.0",
    "react-native-web": "~0.18.12"
  },
  "devDependencies": {
    "@autotoor/eslint-config": "*",
    "@babel/core": "^7.19.3",
    "@testing-library/react-native": "^11.4.0",
    "@types/react": "~18.0.28",
    "@types/react-dom": "~18.0.11",
    "@types/react-native": "~0.70.6",
    "babel-preset-expo": "~9.3.0",
    "jest": "^29.4.3",
    "jest-expo": "^48.0.1"
  },
  "eslintConfig": {
    "extends": "@autotoor/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  },
  "jest": {
    "preset": "jest-expo"
  }
}
